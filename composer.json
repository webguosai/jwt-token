{
    "name": "webguosai/jwt-token",
    "description": "laravel auth jwt",
    "support": {
        "issues": "https://github.com/Webguosai/JwtToken/issues",
        "source": "https://github.com/Webguosai/JwtToken"
    },
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "illuminate/support": "~7|~8",
        "firebase/php-jwt": "^6.4"
    },
    "authors": [
        {
            "name": "webguosai",
            "email": "501807312@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webguosai\\JwtToken\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webguosai\\JwtToken\\Providers\\JwtTokenServiceProvider"
            ],
            "aliases": {
                "Jwt": "Webguosai\\JwtToken\\Facades\\Jwt",
                "JwtToken": "Webguosai\\JwtToken\\Facades\\JwtToken"
            }
        }
    },
    "license": "MIT"
}
